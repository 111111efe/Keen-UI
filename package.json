{
  "name": "keen-ui",
  "version": "1.2.1",
  "description": "Keen UI is a Vue.js UI library with a simple API, inspired by Google's Material Design.",
  "main": "dist/keen-ui.js",
  "scripts": {
    "clean": "rimraf ./dist ./lib ./node_modules/.cache",
    "build": "vue-cli-service build --target lib --formats umd,umd-min --name KeenUI --filename keen-ui src/index.js",
    "build:lib": "rimraf ./lib && node build-lib.js",
    "lint": "vue-cli-service lint --no-fix ./src ./stories ./*.js",
    "format": "vue-cli-service lint ./src ./stories ./*.js",
    "test": "npm run lint",
    "postinstall": "patch-package",
    "storybook": "start-storybook -p 6006 --quiet --ci",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephusPaye/Keen-UI.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "material design",
    "components",
    "ui"
  ],
  "files": [
    "dist",
    "lib",
    "src",
    "CHANGELOG.md",
    "LICENCE",
    "README.md",
    "yarn.lock"
  ],
  "author": {
    "name": "Josephus Paye II"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JosephusPaye/Keen-UI/issues"
  },
  "homepage": "https://github.com/JosephusPaye/Keen-UI/",
  "dependencies": {
    "autosize": "^3.0.20",
    "deepmerge": "^2.0.1",
    "fuzzysearch": "^1.0.3",
    "lodash.debounce": "^4.0.8",
    "tippy.js": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/vue": "^5.2.8",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "execa": "^3.4.0",
    "patch-package": "^6.2.0",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.5.4",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
