{
  "name": "keen-ui",
  "version": "0.8.0",
  "description": "Collection of essential UI components written with Vue and inspired by Material Design.",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack --config webpack.config.js",
    "build-docs": "mkdir docs && cp ./src-docs/index.html docs/index.html && webpack --progress --hide-modules --config webpack.docs.js",
    "build": "webpack --progress --hide-modules --config webpack.build.js",
    "build-min": "webpack --progress --hide-modules --config webpack.build.min.js",
    "clean-dist": "rm -r ./dist",
    "clean-docs": "rm -r ./docs",
    "test": "karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephusPaye/keen-ui.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue-component",
    "material",
    "design",
    "components"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "Josephus Paye II",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JosephusPaye/keen-ui/issues"
  },
  "homepage": "http://josephuspaye.github.io/keen-ui/",
  "dependencies": {
    "dominus": "^5.0.6",
    "horsey": "^3.0.0",
    "tether-drop": "^1.4.2",
    "validatorjs": "^2.0.5",
    "vue": "^1.0.21"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^5.8.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-html": "^1.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^2.1.3",
    "phantomjs-prebuilt": "^2.1.7",
    "stylus-loader": "^1.5.1",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^8.0.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.14"
  }
}
